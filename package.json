{
  "name": "barber",
  "version": "1.0.0",
  "description": "Buy Used Cars in India, New Cars, Sell Cars, Car Prices,Comparisons, Reviews,New Cars Prices 2016",
  "main": "src/index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch server exec node server/bundle.js",
    "dev:build-server": "webpack --config config/dev/webpack.dev.server.js --watch",
    "dev:build-client": "webpack --config config/dev/webpack.dev.client.js --watch",
    "dev:build-sass": "node-sass --watch --source-map true public/scss -o public/css --output-style",
    "build": "npm-run-all --parallel prod:*",
    "prod:build-server": "webpack --config config/prod/webpack.prod.server.js",
    "prod:build-client": "webpack --config config/prod/webpack.prod.client.js",
    "prod:build-sass": "node-sass public/scss -o public/css --output-style compressed",
    "test": "node scripts/test.js --env=jsdom --coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/helpers/**/*.{js,jsx,mjs}",
      "!src/App.js",
      "!src/index.js",
      "!src/client/**/*.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "repository": {
    "type": "git",
